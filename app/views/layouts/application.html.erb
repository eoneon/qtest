<!DOCTYPE html>
<html>
  <head>
    <title>Qtest</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <ul class="nav nav-tabs">
        <li><%= link_to "Invoices", invoices_path %></li>
        <li><%= link_to "Categories", categories_path %></li>
        <li><%= link_to "Artists", artist_types_path %></li>
        <li><%= link_to "Mountings", mount_types_path %></li>
        <li><%= link_to "ItemTypes", item_types_path %></li>
        <li><%= link_to "Edition", edition_types_path %></li>
        <li><%= link_to "Signatures", sign_types_path %></li>
        <li><%= link_to "Certificates", cert_types_path %></li>
        <li><%= link_to "Dimensions", dim_types_path %></li>
        <li><%= link_to "Disclaimers", disclaimer_types_path %></li>
        <li><%= link_to "Fields", item_fields_path %></li>
        <li><%= link_to "Values", field_values_path %></li>
      </ul>
      <%= yield %>
    </div>
  </body>
</html>
